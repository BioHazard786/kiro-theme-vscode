  "version": "1.0.0",
  "publisher": "MohdZaid",
  "author": {
    "name": "Mohd Zaid",
    "email": "message@zaid.qzz.io",
    "url": "https://zaid.qzz.io"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark",
    "light",
    "color-theme",
    "kiro",
    "modern",
    "vibrant"
  ],
  "license": "MIT",
  "homepage": "https://github.com/BioHazard786/kiro-theme-vscode/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/BioHazard786/kiro-theme-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/BioHazard786/kiro-theme-vscode/issues",
    "email": "message@zaid.qzz.io"
  },
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#7138cc",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Kiro Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/kiro-dark.json"
      },
      {
        "label": "Kiro Light",
        "uiTheme": "vs",
        "path": "./themes/kiro-light.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "echo 'No compilation needed for theme'",
    "package": "vsce package",
    "publish": "vsce publish"
